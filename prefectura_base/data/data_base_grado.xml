<odoo>
    <data noupdate="1">
        <!-- Catálogos -->
        <record id="pf_catalogo_grado" model="pf.catalogo">
            <field name="name">Grado</field>
            <field name="descripcion">Catálogo grado de tipo de discapacidad</field>
        </record>
        
        <record id="estado_civil_soltero" model="pf.items">
            <field name="name">LEVE</field>
            <field name="descripcion">Discapacidad Leve</field>
            <field name="catalogo_id" ref="pf_catalogo_grado"/>
        </record>
        <record id="estado_civil_casado" model="pf.items">
            <field name="name">Grave</field>
            <field name="descripcion">Discapacidad Grave</field>
            <field name="catalogo_id" ref="pf_catalogo_grado"/>
        </record>
        
    </data>
</odoo>